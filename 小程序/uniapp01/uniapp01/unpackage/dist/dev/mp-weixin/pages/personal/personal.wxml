<view class="personal"><view class="header"><block wx:if="{{userInfo.nickName}}"><image class="userimg" src="{{userInfo.avatarUrl}}"></image><text>{{userInfo.nickName}}</text><button data-event-opts="{{[['tap',[['exit',['$event']]]]]}}" class="exit" bindtap="__e">退出登录</button></block><block wx:else><image class="userimg" src="../../static/images/weidenglu.jpeg"></image><view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" class="login" bindtap="__e">去登录</view></block></view></view>